{
  "name": "react-ogl",
  "version": "0.1.0",
  "description": "A barebones react renderer for OGL.",
  "bugs": {
    "url": "https://github.com/pmndrs/react-ogl/issues"
  },
  "homepage": "https://github.com/pmndrs/react-ogl#readme",
  "repository": "pmndrs/react-ogl",
  "keywords": [
    "react",
    "renderer",
    "webgl",
    "ogl",
    "shaders"
  ],
  "author": "Cody Bennett (https://github.com/codyjasonbennett)",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.108",
    "@swc/helpers": "^0.2.14",
    "@swc/jest": "^0.2.5",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-native": "^0.66.3",
    "@types/react-reconciler": "^0.26.4",
    "@types/scheduler": "^0.16.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "chokidar": "^3.5.2",
    "concurrently": "^6.3.0",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.3.1",
    "ogl": "^0.0.76",
    "ogl-typescript": "^0.1.40",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-native": "^0.66.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "expo-gl": "^11.0.3",
    "react-merge-refs": "^1.1.0",
    "react-reconciler": "^0.26.2",
    "react-use-measure": "^2.0.4"
  },
  "peerDependencies": {
    "ogl": ">=0.0.72",
    "react": ">=17.0",
    "react-dom": ">=17.0",
    "react-native": ">=0.63.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "scripts": {
    "dev": "concurrently \"swc src -d dist -w\" \"npm run --prefix demo dev\"",
    "prebuild": "rimraf dist",
    "build": "swc src -d dist",
    "postbuild": "tsc && node patch-targets",
    "test": "jest",
    "lint": "prettier src --check && eslint src/**/*.{ts,tsx}",
    "lint-fix": "prettier src --write && eslint --fix src/**/*.{ts,tsx}",
    "prepublish": "npm run build",
    "publish": "npm publish dist"
  }
}
